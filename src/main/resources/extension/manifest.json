{
    "name": "wsName",
    "version": "1.0.0",
    "description": "wsDescription",
    "manifest_version": 3,
    "author": "Scurto",
    "action":{
        "default_popup": "index.html",
        "default_title": "wsDefaultTitle"
    },
    "content_scripts": [
        {
          "matches": [
            "https://*/*",
            "http://*/*"
          ],
          "js": [
            "script.js"
          ]
        }
      ],
    "web_accessible_resources": [
    {
        "matches": [
        "<all_urls>"
        ],
        "resources": [
            "./js/jquery-3.6.0.min.js",
            "./js/stomp.umd.min.js"
        ]
    }
    ],
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "permissions": ["tabs"]
}